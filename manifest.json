{
    "manifest_version": 3,
    "name": "Font Changer By Tejas",
    "version": "1.0",
    "description": "Changes the font of webpages using Chrome storage",
    "permissions": ["storage", "activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }